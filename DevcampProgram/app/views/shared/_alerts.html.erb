<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Gabriel Wilkins Portfolio", sticky: false, time: 500) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Gabriel Wilkins Portfolio", sticky: false, time: 500) %>
<% else %>
  <%= js add_gritter(flash[:notice], title: "Gabriel Wilkins Portfolio", sticky: false, time: 500) %>
<% end %>